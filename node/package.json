{
  "name": "node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node bin/www.js",
    "start:dev": "nodemon bin/www.js",
    "migrate:make": "npm run knexflags migrate:make",
    "migrate:latest": "npm run knexflags --debug migrate:latest",
    "migrate:rollback": "npm run knexflags migrate:rollback",
    "migrate:up": "npm run knexflags migrate:up",
    "migrate:down": "npm run knexflags migrate:down",
    "migrate:list": "npm run knexflags migrate:list",
    "knexflags": "knex --esm --knexfile ./database/knexfile.cjs"
  },
  "type": "module",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "knex": "^0.21.1",
    "morgan": "~1.9.1",
    "pg": "^8.2.1"
  }
}
